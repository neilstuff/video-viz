{
  "name": "electron-viz",
  "version": "1.0.0",
  "description": "Video Index Utility",
  "main": "main.js",
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "electron-builder --publish never",
    "start": "electron ."
  },
  "build": {
    "asarUnpack": [
      "**/app/node_modules/ffmpeg-static/*",
      "**/app/node_modules/ffprobe-static/*"
    ],
    "asar": false,
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "repository": "https://github.com/electron/electron-viz",
  "keywords": [
    "Azure",
    "Video",
    "Electron"
  ],
  "author": "thesourorange",
  "license": "CC0-1.0",
  "dependencies": {
    "file-url": "~2.0.2",
    "form-data": "~2.3.1",
    "path": "~0.12.7",
    "pug": "^2.0.4",
    "mime": "^2.4.4",
    "url": "^0.11.0",
    "electron-json-storage": "^4.1.8",
    "electron-disable-file-drop": "^1.0.2",
    "jquery": "^3.3.1",
    "swiper": "^5.3.7",
    "file-upload-thumbnail": "0.1.0",
    "font-awesome": "^4.7.0",
    "uuid": "^3.2.1",
    "ffmpeg-static": "^4.2.0",
    "fluent-ffmpeg": "2.1.2",
    "ffprobe-static": "^3.0.0",
    "video.js": "^7.6.6"
  },
  "devDependencies": {
    "electron": "^8.2.4",
    "electron-builder": "^22.5.1"
  }
}
